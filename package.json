{
  "name": "classroom-assistant",
  "productName": "Classroom Assistant",
  "version": "2.0.4",
  "description": "Desktop helper for GitHub Classroom",
  "main": "./.webpack/main",
  "scripts": {
    "start": "NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-rebuild && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js .",
    "test-unit": "cross-env NODE_ENV=test TEST_ENV=1 ELECTRON_NO_ATTACH_CONSOLE=1 xvfb-maybe --server-args='-screen 0 1024x768x24' --auto-servernum -- electron-mocha NODE_ENV=test --renderer -t 15000 --require @babel/register --require ignore-styles --require ./test/setup 'app/**/*-test.js' --recursive",
    "test": "npm run lint && npm run test-unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/education/classroom-assistant/"
  },
  "keywords": [
    "GitHub",
    "Classroom",
    "Helper",
    "Education",
    "School",
    "Repository"
  ],
  "author": "GitHub, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/education/classroom-assistant/issues"
  },
  "homepage": "https://github.com/education/classroom-assistant",
  "engines": {
    "node": ">= 8"
  },
  "config": {
    "forge": "./forge.config.js"
  },
